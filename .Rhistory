optopext <- read.csv("~/Downloads/optoprobext.csv", header = TRUE)
optopext
library(ggplot2)
optopext %>%
dplyr::mutate(treatment = paste0(Species,Group)) %>%
ggplot(aes(fill = factor(treatment, levels = c("D.virilisControl","D.virilisExperimental","D.melanogasterControl","D.melanogasterExperimental")),
x = factor(Species, levels= c("D.melanogaster","D.virilis")),
y = Percentage)) + geom_bar(position = "dodge", stat = "identity") +
theme_bw() + ylim(0,100) + scale_fill_manual(values = c("palegreen3", "darkgreen","skyblue","blue")) +
theme(axis.title.x = element_blank(), legend.position = c(.3,.85),
legend.title = element_blank(), text=element_text(size=14),
axis.text.x = element_text(angle = 0, vjust = 0, hjust = 0.5, face = "italic")) +
labs(y = "% Flies Extending Proboscis with Light On", title = "Proboscis Extensions")
